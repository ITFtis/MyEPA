@{
    Layout = "~/Views/_Layout.cshtml";
}


<div class="text-center">
    <br />
    <h5>病媒防治業</h5>
    <br />
</div>

<font size="2">
    <table class="table table-bordered table-striped">
        <tr>
            <th>公司名稱</th>
            <th>縣市</th>
            <th>鄉鎮</th>
            <th>地址</th>
            <th>電話</th>
            <th>傳真</th>
            <th>證書號碼</th>
            <th>負責人</th>
            <th>更新時間</th>
        </tr>
        @foreach (var Item in ViewBag.Data)
        {
            <tr>
                <td>@Item.CompanyName</td>
                <td>@Item.City</td>
                <td>@Item.Town</td>
                <td>@Item.Address</td>
                <td>@Item.Phone</td>
                <td>@Item.Fax</td>
                <td>@Item.License</td>
                <td>@Item.ContactPerson</td>
                <td>@Item.UpdateTime</td>
            </tr>
        }
    </table>
    <br /><br />
</font>
        function drawShape(kind, idx) {

            if (shapes[kind].shape[idx] != null && !confirm("是否重繪？")) {

                return;
            } else {

                if (shapes[kind].shape[idx] != null) {

                    gMap.removeLayer(shapes[kind].shape[idx]);

        /* 計算 Area (平方公尺 => 公頃) */
        function totalArea(shp) {
            var area = 0;

            for (var i = 0; i < shp.length; i++) {
                if (shp[i] != null) {
                    area += shp[i];
                }
            }
            return area;
            //           return shp;
        }
        /*計算讀取的面積*/
        function totalArea2(shp) {
            var area2 = 0;

            for (var i = 0; i < shp.length; i++) {
                if (shp[i] != null) {
                    area2 += L.GeometryUtil.geodesicArea(shp[i].getLatLngs());

                }
            }

            return area2 / 10000;
            //           return shp;
        }

@{
    Layout = "~/Views/_Layout.cshtml";
}
<br />
@Html.ActionLink("返回", "C1a", "CleanerxResource", "", new { @class = "btn btn-primary fa fa-arrow-circle-left", @style = "color:white;padding:4px" })
<br/><br />
<font size="2">
    <table class="table table-bordered table-striped " style="border: 1px solid gray;">
        <tr>
            <th class="text-center">編輯</th>
            @*<th>編號</th>*@
            <th>聯繫單位</th>
            <th>縣市</th>
            <th>鄉鎮市</th>
            <th>X位置</th>
            <th>Y位置</th>
            <th>地址</th>
            <th>臨時堆置場</th>
            <th>更新時間</th>
        </tr>
        @foreach (var Item in ViewBag.Data)
        {

            <tr>

                <td>@Item.ContactUnit</td>
                <td>@Item.City</td>
                <td>@Item.Town</td>
                <td>@Item.Xpos</td>
                <td>@Item.Ypos</td>
                <td>@Item.Address</td>
                <td>@Item.IsDump</td>
                <td>@Item.UpdateTime</td>

            </tr>

        }
    </table>

    <br />
    <br />
</font>
